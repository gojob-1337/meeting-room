{
  "name": "roombelt",
  "version": "1.8.1",
  "private": true,
  "engines": {
    "node": "10.x",
    "yarn": "1.x"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dev:frontend": "cd frontend && cross-env PORT=3001 REACT_APP_VERSION=$npm_package_version BROWSER=none react-scripts start",
    "dev:backend": "cross-env PORT=3001 NODE_ENV=development nodemon --watch backend index.js",
    "test:frontend": "cd frontend && cross-env NODE_PATH=./src react-scripts test",
    "build:frontend": "cd frontend && cross-env GENERATE_SOURCEMAP=false REACT_APP_VERSION=$npm_package_version react-scripts build",
    "heroku-postbuild": "yarn build:frontend",
    "start": "node index.js",
    "docker:build": "cross-env-shell docker build -t ziolko/roombelt:$npm_package_version -t ziolko/roombelt:latest .",
    "docker:publish": "cross-env-shell docker push ziolko/roombelt:$npm_package_version && docker push ziolko/roombelt:latest"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.5.2",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "axios": "^0.19.2",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.10.7",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "create-context": "^1.0.0",
    "cross-env": "^7.0.0",
    "csv-stringify": "^5.3.6",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.2",
    "file-saver": "^2.0.2",
    "googleapis": "^47.0.0",
    "helmet": "^3.21.3",
    "http-proxy-middleware": "^1.0.0",
    "i18next": "^19.3.2",
    "ioredis": "^4.16.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "ms": "^2.1.2",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.2",
    "pg": "^7.18.2",
    "prettier": "1.19.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.11.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-image-fallback": "^8.0.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-select": "^3.0.8",
    "react-table": "^6.10.0",
    "react-test-renderer": "^16.13.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "screenfull": "^5.0.2",
    "sequelize": "5.21.5",
    "simple-oauth2": "^3.3.0",
    "sqlite3": "^4.1.1",
    "store": "^2.0.12",
    "styled-components": "^5.0.1",
    "teedux": "^2.1.5",
    "webfontloader": "^1.6.28",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "typescript": "^3.8.2"
  }
}
