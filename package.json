{
  "name": "roombelt",
  "version": "1.8.0",
  "private": true,
  "engines": {
    "node": "10.x",
    "yarn": "1.19.x"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dev:frontend": "cd frontend && cross-env PORT=3001 REACT_APP_VERSION=$npm_package_version BROWSER=none react-scripts start",
    "dev:backend": "cross-env PORT=3001 NODE_ENV=development nodemon --watch backend index.js",
    "test:frontend": "cd frontend && cross-env NODE_PATH=./src react-scripts test",
    "build:frontend": "cd frontend && cross-env GENERATE_SOURCEMAP=false REACT_APP_VERSION=$npm_package_version react-scripts build",
    "heroku-postbuild": "yarn build:frontend",
    "start": "node index.js",
    "docker:build": "cross-env-shell docker build -t ziolko/roombelt:$npm_package_version -t ziolko/roombelt:latest .",
    "docker:publish": "cross-env-shell docker push ziolko/roombelt:$npm_package_version && docker push ziolko/roombelt:latest"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.5.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.3",
    "axios": "^0.19.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.10.6",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "create-context": "^1.0.0",
    "cross-env": "^6.0.3",
    "csv-stringify": "^5.3.4",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.2",
    "file-saver": "^2.0.2",
    "googleapis": "^46.0.0",
    "helmet": "^3.21.2",
    "http-proxy-middleware": "^0.20.0",
    "i18next": "^11.10.2",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "ms": "^2.1.2",
    "mysql2": "^2.0.2",
    "nodemon": "^2.0.2",
    "pg": "^7.14.0",
    "prettier": "1.19.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
    "react-table": "^6.10.0",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "screenfull": "^5.0.0",
    "sequelize": "5.21.2",
    "simple-oauth2": "^3.1.0",
    "sqlite3": "^4.1.1",
    "store": "^2.0.12",
    "styled-components": "^4.4.1",
    "teedux": "^2.1.5",
    "webfontloader": "^1.6.28",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "typescript": "^3.7.3"
  }
}
